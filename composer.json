{
    "name": "drbyte/viewpages",
    "description": "Support rendering/view of Laravel pages and templates from a database.",
    "keywords": ["laravel", "view", "render", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "DrByte",
            "email": "drbyte@zen-cart.com"
        },
        {
            "name": "Del",
            "email": "del@babel.com.au"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "delatbabel/applog": "dev-master",
        "delatbabel/fluents": "~1.0",
        "delatbabel/site-config": "dev-master",
        "delatbabel/nestedcategories": "dev-master",
        "illuminate/support": "^5.1",
        "illuminate/view": "^5.1",
        "rcrowe/twigbridge": "^0.9.2",
        "twig/twig": "1.*",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DrByte\\ViewPages\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
